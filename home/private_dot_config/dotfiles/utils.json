[
  {
    "name": "1password-cli",
    "description": "Provision secrets across development environments.",
    "url": "https://developer.1password.com/docs/cli/",
    "personal": true,
    "install": {
      "common": "#custom"
    },
    "provides": {
      "common": ["op"]
    }
  },
  {
    "name": "7zip",
    "description": "A file archiver with a high compression ratio.",
    "url": "https://www.7-zip.org/",
    "install": {
      "linux": {
        "debian": {
          "common": "7zip",
          "bullseye": "p7zip",
          "buster": "p7zip"
        },
        "ubuntu": {
          "common": "7zip",
          "focal": "p7zip",
          "bionic": "p7zip",
          "xenial": "p7zip"
        }
      },
      "windows": "#builtin"
    },
    "provides": {
      "linux": {
        "alpine": {
          "common": ["7z"]
        },
        "debian": {
          "sid": ["7zz"],
          "bookworm": ["7zz"],
          "bullseye": ["7zr"],
          "buster": ["7zr"]
        },
        "ubuntu": {
          "latest": ["7zz"],
          "kinetic": ["7zz"],
          "jammy": ["7zz"],
          "focal": ["7zr"],
          "bionic": ["7zr"],
          "xenial": ["7zr"]
        }
      },
      "windows": ["7z"]
    }
  },
  {
    "name": "chezmoi",
    "description": "Manage your dotfiles across multiple diverse machines, securely.",
    "url": "https://chezmoi.io/",
    "install": {
      "common": "#builtin"
    }
  },
  {
    "name": "git",
    "description": "Distributed version control system.",
    "url": "https://git-scm.com/",
    "install": {
      "windows": "#builtin"
    },
    "provides": {
      "common": ["git"],
      "windows": [
        "sh",
        "bash",
        "gitk",
        "git-gui",
        "scalar",
        "tig",
        "git-bash"
      ]
    }
  },
  {
    "name": "jq",
    "description": "A command-line JSON processor that uses a domain-specific language.",
    "url": "https://stedolan.github.io/jq/",
    "install": {
      "linux": {
        "common": "#custom"
      }
    }
  },
  {
    "name": "nano",
    "description": "Command-line text editor.",
    "url": "https://nano-editor.org/"
  },
  {
    "name": "openssh",
    "description": "Connectivity tool for remote login with the SSH protocol",
    "url": "https://www.openssh.com/",
    "install": {
      "linux": {
        "debian": {
          "common": "openssh-server"
        },
        "ubuntu": {
          "common": "openssh-server"
        }
      },
      "windows": "#builtin"
    },
    "provides": {
      "common": [
        "ssh",
        "ssh-keygen"
      ]
    }
  }
]
